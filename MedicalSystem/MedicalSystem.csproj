<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>mainIcon.ico</ApplicationIcon>
    <Company>d_cruncher</Company>
    <Title>Умная медицинская система</Title>
    <Description>Информационная система для медицинских организаций на основе нейронной сети</Description>
    <Copyright>23.12.2024</Copyright>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Authors>d_cruncher</Authors>
    <RepositoryUrl>https://github.com/51dollar/neural-networks</RepositoryUrl>
    <Product>Умная медицинская система</Product>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);Умная медицинская система</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="mainIcon.ico" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NeuralNetworks\NeuralNetworks.csproj" />
  </ItemGroup>

</Project>